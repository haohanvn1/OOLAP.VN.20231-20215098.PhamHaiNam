package code;

public class Aims {
public static void main(String[] arg) {
    Cart anOrder = new Cart();
    DigitalVideoDc dvd1 = new DigitalVideoDisc("The Lion King",
            "Animation", " Roger Allers", 87, 19.95f);
    DigitalVideoDc dvd2 = new DigitalVideoDisc("Star Wars",
            "Science Fiction", "George Lucas", 87, 24.95f);
    DigitalVideoDc dvd3 = new DigitalVideoDisc("Aladin", "Animation", 18.99f);

    anOrder.addDigitalVideoDisc(dvd3);
    anOrder.addDigitalVideoDisc(dvd2);
    anOrder.addDigitalVideoDisc(dvd1);


    System.out.println("Total cost is:");
    System.out.println(anOrder.calculateCost());
}

}